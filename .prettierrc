{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "importOrder": ["<BUILTIN_MODULES>", "<THIRD_PARTY_MODULES>", "", "^@/(.*)$", "^[./]", "", "<TYPES>", "<TYPES>^[./]"],
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-sort-json", "@ianvs/prettier-plugin-sort-imports"],
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false
}
